<?xml version="1.0" encoding="utf-8"?>
<Doctrine-mapping xmlns="http://Doctrine-project.org/schemas/orm/Doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://Doctrine-project.org/schemas/orm/Doctrine-mapping http://Doctrine-project.org/schemas/orm/Doctrine-mapping.xsd">
  <entity name="TiposIva" table="tipos_iva">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="float" column="tipo" precision="10" scale="0" nullable="true"/>
    <field name="porcentaje" type="float" column="porcentaje" precision="10" scale="0" nullable="true"/>
  </entity>
</Doctrine-mapping>
