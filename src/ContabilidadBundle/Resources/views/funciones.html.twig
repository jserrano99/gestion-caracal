<script>
    function EliminarAsiento(id) {
            bootbox.confirm("Confirmar el Eliminar Asiento " + id, 
                 function(result){
                     if (result) {
                        var url =Routing.generate("deleteAsiento",{"id":id},true);
                        window.location = url;                     
                    } 
                 });
    }
    
    function EliminarProyecto(id) {
